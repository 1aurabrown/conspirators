<div class="card-container" data-talent="<%=talent.id%>">
  <figure class="talent-card">
    <a class="card-link" href="<%= "/#{talent.slug}" %>">
    </a>
    <figcaption class="card-content">
      <div>
      <p class="talent-card-subtitle"><%= talent.genders.join ' / ' %></p>
      </div>
      <div>
      <p class="talent-card-subtitle"> <%= talent.skills.to_sentence %> </p>
      </div>
      <h1 class="talent-card-title"><%= talent.name %></h1>

    </figcaption>
    <img class="card-img-bottom" src="<%= talent.avatar.url(:large)%>">
    <%= render partial: 'shared/save_button', locals: {talent: talent} %>
  </figure>
</div>