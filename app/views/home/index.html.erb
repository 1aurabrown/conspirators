<article class="home">
  <header id="page-header">
    <div id="header-content">
      <h1 id="page-title">
        <%=render partial: 'tag_selection' %>
      </h1>
    </div>
  </header>
  <div class="page-content">
    <section class="section">
      <div class="section-content talents-section">
        <% @talents.each do | talent | %>
          <%= render partial: 'shared/talent_card', locals: { talent: talent } %>
        <% end %>
      </div>
    </section>
  </div>
</article>